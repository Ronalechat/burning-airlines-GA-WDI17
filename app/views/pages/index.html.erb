<div class="container">
  <h1>Burning Airlines</h1>

<!-- Append results to main -->
<div id="main">
</div>

<script type="text/x-template" id="flights-template">

  <h2>Origin</h2>

  <div class="origin">
    <select id="origin">
      <option>Blank</option>
    </select>
  </div>

  <h2>Destination</h2>

  <div class="desination">
    <select id="#destination">
      <option>Blank</option>
    </select>
  </div>

  <input type="button" id="restart" value="Find">
</script>

<script type="text/x-template" id="flightListView-template">

  <h3></h3>
    <table>
      <thead class="thead">
        <th>Date</th>
        <th>Flight_num</th>
        <th>From > To</th>
        <th>Plane</th>
        <th>Seats</th>
      </thead>
    </table>
</script>

<script type="text/x-template" id="airplane-template">

</script>

<script type="text/x-template" id="search-template">
  <h2>Origin</h2>

  <div class="search_date">
    <form >
  Date: <input type="date" name="date" id="date">
  <br><br>

</form>
  </div>
  <h2>Origin</h2>

  <div class="origin">
    <select id="origin">
      <option name="origin" id="origin">{{=origin}}</option>
    </select>
  </div>

  <h2>Destination</h2>

  <div class="desination">
    <select id="#destination">
      <option name="destination" id="destination">{{=destination}}</option>
    </select>
  </div>

  <input type="button" id="restart" value="Find">
</script>

<script type="text/x-template" id="flightid-template">
  <table class="table table-bordered">
  <tr class="active"><p>Flight:<mark>{{=flight_num}}</mark></p></tr>
  <tr class="active"><p>Origin: <mark>{{=origin}}</mark></p></tr>
  <tr class="active"><p>Destination: <mark>{{=destination}}</mark></p></tr>
  <tr class="active"><p>Date: <mark>{{=date}}</mark></p></tr>
  </table>
</div>
  <div class="redline">
    <!-- nested looop for rows and columns -->
      <div id="seats"></div>
      <!-- order is important, make sure we render first this.$el.find -->
      <!-- flight view render _template to paste variables in-->
    </table>
  </div>

</script>
</div>
